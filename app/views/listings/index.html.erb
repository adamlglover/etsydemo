
<div class="jumbotron">
  <center><h1>Welcome to my site</h1></center>
 
</div>


<div class="row">
  <% @listings.each do |listing| %>

  <div class="col-sm-3">
   <div class="thumbnail">
    <div class="thumbnail">
   <%= link_to image_tag(listing.image_url(:thumb)), listing %>
   </div>
   <div class="row">
  
     <div class="col-md-12"><%= link_to listing.name, listing %></div>
     <div class="col-md-8"><%= listing.user.name %></div>
     <div class="col-md-4"><%= number_to_currency(listing.price) %></div>
<% if user_signed_in?  %>   
  <%= link_to (image_tag "favicon.ico"), pages_about_path %>
     <% end %> 
     </div>
    
     
    
   </div>
    </div>
 
  <% end %>
  </div> 
 
 
   <%= form_tag import_listings_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import CSV" %>   
  <% end %> 

  <%= will_paginate @listings, renderer: BootstrapPagination::Rails %>


 
 